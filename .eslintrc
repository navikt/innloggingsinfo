{
    "parser": "babel-eslint",
    "rules": {
        "linebreak-style": 0,
        "arrow-parens": [2, "always"],
        "indent": [2, 4, {"SwitchCase": 1}],
        "comma-dangle": [2, "never"],
        "max-len": [2, 120, 4],
        "no-unused-vars": [2, {"varsIgnorePattern": "_", "argsIgnorePattern": "_"}]
    },
    "globals": {
        "$": false
    },
    "env": {
        "browser": true
    },
    "extends": "airbnb"
}
